{
  "schemaVersion": 1,
  "id": "cyanlib",
  "version": "${version}",
  "name": "CyanLib",
  "description": "This is a library for small utilities like custom translations for the mods that use this library or config",
  "authors": [
    "Aeldit"
  ],
  "contact": {
    "homepage": "https://modrinth.com/user/Aeldit",
    "sources": "https://github.com/Aeldit/CyanLib",
    "issues": "https://github.com/Aeldit/CyanLib/issues"
  },
  "license": "CC0-1.0",
  "icon": "assets/cyanlib/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "fr.aeldit.cyanlib.core.CyanLibClientCore"
    ],
    "server": [
      "fr.aeldit.cyanlib.core.CyanLibServerCore"
    ],
    "modmenu": [
      "fr.aeldit.cyanlib.core.gui.ModMenuApiImpl"
    ]
  },
  "depends": {
    "fabricloader": ">=0.14.21",
    "fabric-resource-loader-v0": "*",
    "fabric-command-api-v2": "*",
    "minecraft": "1.20.x",
    "java": ">=17"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/JJMnf62Kuj",
        "modmenu.wiki": "https://github.com/Aeldit/CyanLib/wiki"
      },
      "badges": [
        "library"
      ]
    }
  }
}
